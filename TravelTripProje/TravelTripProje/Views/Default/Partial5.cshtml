@using TravelTripProje.Models.Siniflar
@model List<Lunaparklar>
<div class="box1-left">
    @foreach (var x in Model)
    {
        <div class="bot-img">
            <img src="@x.ParkImage" style="width:120px;height:67px" alt="" />
        </div>
        <div class="bot-desc1">
            <p><a href="/Lunapark/LunaparkDetay/@x.ID" style="color:dimgray">@x.Aciklama.Substring(0, 70)...</a> </p>
        </div>
        <div class="clear"></div>
    }

</div>
